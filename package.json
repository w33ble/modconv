{
  "name": "modconv",
  "version": "0.0.1",
  "description": "Module conversion tool for node",
  "bin": "bin/modconv.js",
  "scripts": {
    "lint": "eslint '*.{js,mjs}' 'src/**/*.{js,mjs}' 'bin/**/*.{js,mjs}'",
    "precommit": "lint-staged",
    "version": "npm-auto-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w33ble/modconv.git"
  },
  "keywords": [
    "esm",
    "cjs",
    "umd",
    "amd",
    "systemjs",
    "convert",
    "transpile",
    "module"
  ],
  "files": [
    "bin/",
    "CHANGELOG.md",
    "AUTHORS.md"
  ],
  "author": "Joe Fleming (https://github.com/w33ble)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/w33ble/modconv/issues"
  },
  "homepage": "https://github.com/w33ble/modconv",
  "lint-staged": {
    "ignore": [
      "package.json"
    ],
    "linters": {
      "*.{js,mjs}": [
        "eslint --fix"
      ],
      "*.{js,mjs,json,css}": [
        "prettier --write"
      ]
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-modules-amd": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-modules-systemjs": "^7.2.0",
    "@babel/plugin-transform-modules-umd": "^7.2.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "cross-spawn": "^6.0.5",
    "getopts": "^2.2.3"
  },
  "devDependencies": {
    "@w33ble/npm-auto-tools": "*",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "prettier": "^1.9.0"
  }
}
